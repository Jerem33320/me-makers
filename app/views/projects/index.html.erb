<h1>Tout vos Projets</h1>
<div class="container">
  <% @projects.each do |project| %>
  <p>
    <%= project.title %>
  </p>
  <p>
    <%= project.description %>
  </p>
  <p>
    <%= project.date %>
  </p>
  <p>
    <%= link_to 'Show', project_path(project) %>
    <%= link_to "Retour", new_project_path, class: 'btn btn-light' %>
  </p>
<% end %>
</div>
